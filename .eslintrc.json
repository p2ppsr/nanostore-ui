{
    "root": true,
    "env": {
      "node": true,
      "commonjs": true,
      "es2021": true
    },
    "extends": [
      "plugin:react/recommended",
      "standard-with-typescript"
    ],
    "overrides": [],
    "parserOptions": {
      "project": "./tsconfig.json",
      "ecmaVersion": "latest"
    },
    "plugins": [
      "react"
    ],
    "rules": {
      "@typescript-eslint/no-misused-promises": ["error", {
        "checksVoidReturn": false
      }],
      "indent": ["error", 2],
      "quotes": ["error", "single"],
      "react/prop-types": "off"
    }
  }
  
